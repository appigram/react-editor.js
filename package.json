{
  "name": "@appigram/react-editor-js",
  "version": "1.0.6",
  "description": "Unofficial react component for editorjs (https://editorjs.io/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/appigram/react-editor-js.git"
  },
  "author": "Evgeny Sysmanov (appigram@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appigram/react-editor-js/issues"
  },
  "homepage": "https://github.com/appigram/react-editor-js#readme",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "es",
    "esm",
    "lib"
  ],
  "keywords": [
    "react",
    "editor",
    "editor.js",
    "editorjs",
    "@editorjs",
    "react-editor-js",
    "react-editorjs",
    "editorjs-component",
    "editor-js-component",
    "wysiwyg"
  ],
  "scripts": {
    "build": "npm run build-cjs && npm run build-es && npm run build-esm && npm run build-umd",
    "build-cjs": "tsc --outDir lib --module commonjs --target es5 -d --declarationMap",
    "build-es": "tsc --outDir es --module es2015 --target es2015 -d --declarationMap",
    "build-esm": "tsc --outDir esm --module es2015 --target es5 -d --declarationMap",
    "build-umd": "webpack --mode=production",
    "build-storybook": "build-storybook",
    "contributors-add": "all-contributors add",
    "contributors-generate": "all-contributors generate",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "lint": "tsc --noEmit && eslint '**/*.{ts,tsx}' --quiet",
    "prebuild": "rimraf dist && rimraf es && rimraf esm && rimraf lib",
    "prepublishOnly": "npm run build && npm run prerelease",
    "prerelease": "npm run test",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version",
    "size": "npm run size-build && size-limit",
    "size-build": "npm run build",
    "start": "yarn start-storybook -p 6006 --ci",
    "test": "npm run test-jest && npm run test-cypress",
    "test-jest": "jest",
    "test-cypress": "start-server-and-test start http-get://localhost:6006 cypress:run",
    "watch-test": "jest --watch"
  },
  "peerDependencies": {
    "@editorjs/editorjs": ">=2.18",
    "@editorjs/header": ">=2.5",
    "@editorjs/paragraph": ">=2.7",
    "react": ">=16.13"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@bahmutov/add-typescript-to-cypress": "2.1.2",
    "@cypress/webpack-preprocessor": "5.4.2",
    "@editorjs/checklist": "1.1.0",
    "@editorjs/code": "2.5.0",
    "@editorjs/delimiter": "1.1.0",
    "@editorjs/editorjs": "2.16.1",
    "@editorjs/embed": "2.3.1",
    "@editorjs/header": "2.3.2",
    "@editorjs/image": "2.4.2",
    "@editorjs/inline-code": "1.3.1",
    "@editorjs/link": "2.2.1",
    "@editorjs/list": "1.5.0",
    "@editorjs/marker": "1.2.2",
    "@editorjs/paragraph": "2.6.1",
    "@editorjs/quote": "2.3.0",
    "@editorjs/raw": "2.1.2",
    "@editorjs/simple-image": "1.3.3",
    "@editorjs/table": "1.2.2",
    "@editorjs/warning": "1.1.1",
    "@size-limit/preset-small-lib": "4.5.5",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/core": "5.3.19",
    "@storybook/core-events": "5.3.19",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "5.3.19",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.5",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "all-contributors-cli": "6.16.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "commitizen": "4.1.2",
    "cypress": "4.11.0",
    "cz-conventional-changelog": "3.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.5.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-ns-ts": "1.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.8",
    "fork-ts-checker-webpack-plugin": "5.0.9",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-docgen-typescript-loader": "3.7.2",
    "react-dom": "16.13.1",
    "rimraf": "3.0.2",
    "size-limit": "4.5.5",
    "standard-version": "8.0.2",
    "start-server-and-test": "1.11.2",
    "storybook-readme": "5.0.8",
    "terser-webpack-plugin": "3.0.7",
    "ts-jest": "26.1.3",
    "ts-loader": "8.0.1",
    "typescript": "3.9.7",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --write",
      "git update-index --again",
      "jest --findRelatedTests"
    ]
  },
  "size-limit": [
    {
      "limit": "6 KB",
      "path": "dist/**/*.js",
      "config": "./webpack.config.js",
      "ignore": [
        "react",
        "react-dom"
      ]
    },
    {
      "limit": "6 KB",
      "webpack": false,
      "path": "lib/**/*.js"
    },
    {
      "limit": "6 KB",
      "webpack": false,
      "path": "es/**/*.js"
    },
    {
      "limit": "6 KB",
      "webpack": false,
      "path": "esm/**/*.js"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "changelogHeader": "# react-editor-js\n\nAll notable changes to this project will be documented here. The format is based on [Keep a Changelog](http://keepachangelog.com/en/1.0.0/) and this project adheres to [Semantic Versioning](http://semver.org/spec/v2.0.0.html).\n\n",
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ]
  }
}
